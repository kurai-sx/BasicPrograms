# Counting Upper and Lower case alphabets in a sentence


'''def me(a):
    
    upper = 0
    lower = 0
    
    for i in  a:
        if i.isupper():
            upper += 1
        elif i.islower():
            lower +=1
        else:
            pass
    print("Upper : ", upper)
    print("Lower : ",lower)
me("I Love My India")'''


#print fibonnachi number



'''

def num(n):
    a = 0
    b = 1
    
    
    if n == 1 or n == 2:
        print(b)
    else:
        print(a)
        print(b)
        for i in range(2,n):
            c = a + b
            a = b
            b = c
            print(c)
   
   
num(9) '''


#Find 15th position in python

'''def num(n):
    a = 0
    b = 1
    c = 0
    
    if n == 1 and n == 2:
        print(b)

    else:
        for i in range(0,n-1):
            c = a + b
            a = b
            b = c
        
        print(c)
    
num(15)
'''

