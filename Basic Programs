# Factorial of a number

'''a = int(input("Enter your number : "))
fac = 1
for i in range(1, a+1):
    fac = fac * i
print(fac)'''

#  Permutation of Given number

'''a = int(input("Enter your number : "))
fac = 1
fac1 = 1
def num():
    fac = 1
    fac1 = 1
    for i in range(1, a+1):
        fac = fac * i
    print(fac)
    for j in range(1,a):
        fac1 = fac1 * j
    print(fac1)
    print("Permutation of given number is : ", int(fac/fac1))
num()'''


       
# Combinations of Given number

'''a = int(input("Enter your number : "))
r = int(input("Enter your number : "))

def num():
    if 0 <= r and r <= a:

        fac = 1
        fac1 = 1
        fac2 = 1
        for i in range(1,a+1):
            fac = fac * i
        print(fac)

        for j in range(1, r+1):
            fac1 = fac1 * j
        print(fac1)

        for k in range(2):
            fac2 = (a-r) * k
        print(fac2)

        print("Combinations of the given numbers are : ", int(fac/(fac1 * fac2)))
    else:
        print("Invalid Syntax")
num()'''


# Simple calculator for user defined values

'''
def cal():
    a = int(input("Enter your 1st number : "))
    c = input("Tell me what to perform : ")
    b = int(input("Enter your 2nd number : "))
    if c == '+':
        print(a+b)
    elif c == '-':
        print(a-b)
    elif c == '*':
        print(a*b)
    elif c == '/':
        print(int(a/b))
    else:
        print("Invalid Syntax !")
cal()'''

